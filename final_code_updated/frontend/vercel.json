{
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "functions": {
    "app/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "env": {
    "REACT_APP_API_URL": "https://api-server-wcjc.onrender.com"
  },
  "build": {
    "env": {
      "REACT_APP_API_URL": "https://api-server-wcjc.onrender.com"
    }
  },
  "dev": {
    "env": {
      "REACT_APP_API_URL": "https://api-server-wcjc.onrender.com"
    }
  }
} 